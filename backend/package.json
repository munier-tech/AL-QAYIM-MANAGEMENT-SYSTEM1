    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "redis": "^5.6.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10"
